#: main.c:284
msgid "  -A <alias>    Print an expanded version of the given alias to stdout and exit"
msgstr "  -A <alias>    Skriv ut en utvidet versjon av angitt alias til stdout og avslutt"

#: main.c:285
msgid ""
"  -a <file>     Attach one or more files to a message (must be the last option)\n"
"                Add any addresses after the '--' argument"
msgstr ""
"  -a <file>     Legg til én eller flere filer i en melding (må være siste valg)\n"
"                Legg til adresser etter '--' -argumentet"

#: main.c:287
msgid "  -B            Run in batch mode (do not start the ncurses UI)"
msgstr "  -B            Kjør i knippe-modus (ikke start ncurses-grensesnittet)"

#: main.c:288
msgid "  -b <address>  Specify a blind carbon copy (Bcc) recipient"
msgstr "  -b <adresse>  Angi en blindkopi (Bcc)-mottager"

#: main.c:289
msgid "  -c <address>  Specify a carbon copy (Cc) recipient"
msgstr "  -c <adresse>  Angi en kopi (Cc)-mottager"

#: main.c:290
msgid "  -D            Dump all config variables as 'name=value' pairs to stdout"
msgstr "  -D            Dump alle oppsettsvariabler som «navn=verdi»-par til standard utdata"

#: main.c:291
msgid "  -D -O         Like -D, but show one-liner documentation"
msgstr "  -D -O         Som -D, men vis énlinjesdokumentasjon"

#: main.c:292
msgid "  -D -S         Like -D, but hide the value of sensitive variables"
msgstr "  -D -S         Som -D, men skjul verdien av sensitive variabler"

#: main.c:293
msgid ""
"  -d <level>    Log debugging output to a file (default is \"~/.neomuttdebug0\")\n"
"                The level can range from 1-5 and affects verbosity"
msgstr ""
"  -d <nivå>    Logg avlusingsinfo til en fil (forvalget er \"~/.neomuttdebug0\")\n"
"                Nivå fra 1-5, som igjen har innvirkning på ordrikhet"

#: main.c:295
msgid "  -E            Edit draft (-H) or include (-i) file during message composition"
msgstr "  -E            Rediger kladd (-H) eller inkluder (-i) fil under forfatting av melding"

#: main.c:296
msgid "  -e <command>  Specify a command to be run after reading the config files"
msgstr "  -e <kommando>  Angi en kommando som skal kjøres etter lesing av oppsettsfilene"

#: main.c:297
msgid "  -F <config>   Specify an alternative initialization file to read"
msgstr "  -F <oppsett>   Angi en alternativ oppsettsfil å lese"

#: main.c:298
msgid "  -f <mailbox>  Specify a mailbox (as defined with 'mailboxes' command) to load"
msgstr "  -f <e-postboks>  Angi en e-postboks (som definert med «mailboxes»-kommandoen for å laste inn"

#: main.c:299
msgid "  -G            Start NeoMutt with a listing of subscribed newsgroups"
msgstr "  -G            Start NeoMutt med en opplisting av abonnerte nyhetsgrupper"

#: main.c:300
msgid "  -g <server>   Like -G, but start at specified news server"
msgstr "  -g <tjener>   Som -G, men start på angitt nyhetstjener"

#: main.c:301
msgid "  -H <draft>    Specify a draft file with header and body for message composing"
msgstr "H <kladd>    Angi en malfil med hode og kropp for meldingsforfatting"

#: main.c:302
msgid "  -h            Print this help message and exit"
msgstr "  -h            Skriv ut denne hjelpemeldingen og avslutt"

#: main.c:303
msgid "  -i <include>  Specify an include file to be embedded in the body of a message"
msgstr "  -i <inkluder>  Angi inkluderingsfil å innebygge i kroppen av en melding"

#: main.c:304
msgid "  -l <file>     Specify a file for debugging output (default \"~/.neomuttdebug0\")"
msgstr "  -l <file>     Angi en fil for avlusingsutdata, (forvalget er «~/.neomuttdebug0»)"

#: main.c:305
msgid ""
"  -m <type>     Specify a default mailbox format type for newly created folders\n"
