#. L10N: Try to limit to 80 columns.  If more space is needed add an indented line
#: main.c:271
msgid "options:"
msgstr "Nastavenia kompilácie:"

#: main.c:272
msgid ""
"  --            Special argument forces NeoMutt to stop option parsing and treat\n"
"                remaining arguments as addresses even if they start with a dash"
msgstr ""
"  --            Speciálnú prepínač, ktorý spôsoví, že NeoMutt nebude zostávajúce parametre\n"
"                pokládať za prepínač, ale za adresy, i keď budú začínať pomlčkou"

#: main.c:274
msgid "  -A <alias>    Print an expanded version of the given alias to stdout and exit"
msgstr "  -A <alias>    Vypíše expandovaný alias na štandardný výstup a skončí"

#: main.c:275
msgid ""
"  -a <file>     Attach one or more files to a message (must be the last option)\n"
"                Add any addresses after the '--' argument"
msgstr ""
"  -a <súbor>    Pripojí k správe súbor(y), musí to byť posledný prepínač\n"
"                Další adresy lze přidat až za přepínač „--“"

#: main.c:277
msgid "  -B            Run in batch mode (do not start the ncurses UI)"
msgstr "  -B            Dávkový režim (nespustí sa užívateľské rozhranie)"

#: main.c:278
msgid "  -b <address>  Specify a blind carbon copy (Bcc) recipient"
msgstr "  -b <adresa>   Určuje adresu pre utajenú kópiu (Bcc)"

#: main.c:279
msgid "  -c <address>  Specify a carbon copy (Cc) recipient"
msgstr "  -c <address>  Určuje adresu pre kúpiu (Cc)"

#: main.c:280
msgid "  -C            Enable Command-line Crypto (signing/encryption)"
msgstr "  -C            Zapnúť šifrovacie funkcie v prikazovom riadku (signing/encryption)"

#: main.c:281
msgid "  -D            Dump all config variables as 'name=value' pairs to stdout"
msgstr "  -D            Na štandardný výstup vypíše hodnoty všetkých premenných"

#: main.c:282
msgid "  -D -O         Like -D, but show one-liner documentation"
msgstr "  -D -S         Podobné ako -D, navyše ukáže jednoriadkovú dokumentáciu"

#: main.c:283
msgid "  -D -S         Like -D, but hide the value of sensitive variables"
msgstr "  -D -O         Podobné ako -D, navyše schová hodnoty citlivých premenných"

#: main.c:284
msgid ""
"  -d <level>    Log debugging output to a file (default is \"~/.neomuttdebug0\")\n"
"                The level can range from 1-5 and affects verbosity"
msgstr ""
"  -d <level>    Ladiace informácie zapisuje do „~/.neomuttdebug0“ \n"
"                Hodnota 1-5 ovlivnňuje úroveň podrobnosti"

#: main.c:286
msgid "  -E            Edit draft (-H) or include (-i) file during message composition"
msgstr "  -E            Upraví koncept (-H) alebo vloží (-i) súbor do tela správy"

#: main.c:287
msgid "  -e <command>  Specify a command to be run after reading the config files"
msgstr "  -e <command>  Príkaz bude vykonaný po inicializácii a načítanie konfigurácie"

#: main.c:288
msgid "  -F <config>   Specify an alternative initialization file to read"
msgstr "  -F <config>   Alternatívne súbor s konfiguráciou (namiesto „neomuttrc“)"

#: main.c:289
msgid "  -f <mailbox>  Specify a mailbox (as defined with 'mailboxes' command) to load"
msgstr "  -f <mailbox>  Načíta z tejto schránky (definované v „Mailboxes“)"

#: main.c:290
msgid "  -G            Start NeoMutt with a listing of subscribed newsgroups"
msgstr "  -G            Ponúkne výber diskusných skupín (NNTP)"

#: main.c:291
msgid "  -g <server>   Like -G, but start at specified news server"
msgstr "  -g <server>   Podobné ako -G, naviac určí server diskusných skupín (NNTP)"

#: main.c:292
msgid "  -H <draft>    Specify a draft file with header and body for message composing"
msgstr "  -H <draft>    Zo súboru s konceptom budú načítané hlavičky a telo správy"

#: main.c:293
msgid "  -h            Print this help message and exit"
msgstr "  -h            Vypíše túto nápovedu a skončí"

#: main.c:294
msgid "  -i <include>  Specify an include file to be embedded in the body of a message"
msgstr "  -i <include>  Tento súbor bude vložený do tela správy"

#: main.c:295
msgid "  -l <file>     Specify a file for debugging output (default \"~/.neomuttdebug0\")"
msgstr "  -l <file>     Určí súbor pre ukladanie ladiacich informácií (predvolené „~/.neomuttdebug0“)"

#: main.c:296
msgid ""
"  -m <type>     Specify a default mailbox format type for newly created folders\n"
"                The type is either MH, MMDF, Maildir or mbox (case-insensitive)"
msgstr ""
"  -m <type>     Určí predvolený typ schránky pre novo vytvárané zložky \n"
"                Typy: MH, MMDF, Maildir alebo mbox (veľkosť písmen nerozhoduje) "

#: main.c:298
msgid "  -n            Do not read the system-wide configuration file"
msgstr "  -n            NeoMutt nebude čítať systémový konfiguračný súbor"

#: main.c:299
msgid "  -p            Resume a prior postponed message, if any"
msgstr "  -p            Vráti sa k odloženej rozpísané správe, ak existuje"

#: main.c:300
msgid ""
"  -Q <variable> Query a configuration variable and print its value to stdout\n"
"                (after the config has been read and any commands executed)\n"
"                Add -O for one-liner documentation"
msgstr ""
"  -Q <variable> Dotaz na konfiguračnú premennú a jej hodnotu vypísať na štandardný výstup\n"
"                (po načítaní konfigurácie a vykonaní všetkých príkazov)\n"
"                Pridať -O pre jednorázovú dokumentáciu"

#: main.c:303
msgid "  -R            Open mailbox in read-only mode"
msgstr "  -R            Otvor schránku v režime iba na čítanie"

#: main.c:304
msgid "  -s <subject>  Specify a subject (must be enclosed in quotes if it has spaces)"
msgstr "  -s <subject>  Špecifikuje predmet (ak obsahuje mezery, musí byť vo uvozovkách)"

#: main.c:305
msgid "  -v            Print the NeoMutt version and compile-time definitions and exit"
msgstr " -v             Zobrazí verziu NeoMuttu a parametre zadané pri preklade a skončí"

#: main.c:306
msgid "  -vv           Print the NeoMutt license and copyright information and exit"
msgstr "  -vv           Zobrazí licenciu NeoMuttu a oprávnenia na použitie a skončí"

#: main.c:307
msgid "  -y            Start NeoMutt with a listing of all defined mailboxes"
msgstr "  -y            Ponúkne k vybranie schránku uvedenú v zozname „Mailboxes“"

#: main.c:308
msgid ""
"  -Z            Open the first mailbox with new message or exit immediately with\n"
"                exit code 1 if none is found in all defined mailboxes"
msgstr ""
"  -Z            Otvorí prvú schránku s novou poštou; ak nie je žiadna nová nájdená,\n"
"                ihneď skončí s kódom 1 "

#: main.c:310
msgid ""
"  -z            Open the first or specified (-f) mailbox if it holds any message\n"
